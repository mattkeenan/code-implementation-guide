#!/bin/sh
# Script: cig-load-autoload-config
# Version: v0.1.1-5-gcea1c19
# Source: https://github.com/OWNER/REPO
# Purpose: Loads .cig/autoload.yaml configuration file or provides fallback message
# Replaces: cat .cig/autoload.yaml 2>/dev/null || echo "No autoload config found"

if [ -f ".cig/autoload.yaml" ]; then
    cat ".cig/autoload.yaml"
else
    echo "No autoload config found"
fi