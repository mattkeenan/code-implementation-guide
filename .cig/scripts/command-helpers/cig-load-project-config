#!/bin/sh
# Script: cig-load-project-config
# Version: v0.1.1-5-gcea1c19
# Source: https://github.com/OWNER/REPO
# Purpose: Loads implementation-guide/cig-project.json configuration or provides fallback
# Replaces: cat implementation-guide/cig-project.json 2>/dev/null || echo "No project config found"

if [ -f "implementation-guide/cig-project.json" ]; then
    cat "implementation-guide/cig-project.json"
else
    echo "No project config found"
fi